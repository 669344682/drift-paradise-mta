{
    "resourcePrefix": "dp",

    "enableScriptCache": false,
    "enableShaderCache": false,
    "enableCompilation": true,
    "enablePathEncrypt": true,
    "enableExportEncrypt": false,
    "enableReadmeFiles": false,

    "only": ["dpWebAPI"],
    "include": [
        "geoip",        
        "blur_box",
        "car_reflections",
        "water_reflections",
        "dynamic_lighting",
        "dynamic_lighting_vehicles",
        "shader_dynamic_sky",
        
        "server_assets",
        "MAPPING-MAIN",
        "TD-INT",
        "TD-MAPFILES",
        "TD-RACEMAPS",
        "TX-VILLA",
    ],

    "pathEncryptExclude": [
        "mta-helper.fx",
        "tex_matrix.fx",
        "light1.fx",
        "light0.fx",
        "common.fx",
        ".json",
        ".txt",
        ".map",
        "_DPASSET",
        "@"
    ]
}